<plugin name='lesscss-resources' version='1.3.0.3' grailsVersion='1.3.5 &gt; *'>
  <author>Paul Fairless</author>
  <title>Less CSS Resources</title>
  <description>\
This plugin supports server-side compilation of .less CSS files to their .css counterparts.
</description>
  <documentation>https://github.com/paulfairless/grails-lesscss-resources</documentation>
  <type>LesscssResourcesGrailsPlugin</type>
  <resources>
    <resource>LesscssResourceMapper</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://plugins.grails.org' />
    <repository name='http://repo.grails.org/grails/plugins' url='http://repo.grails.org/grails/plugins/' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='grailsCore' url='http://svn.codehaus.org/grails/trunk/grails-plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
    <repository name='http://grails.org/plugins' url='http://grails.org/plugins' />
  </repositories>
  <dependencies>
    <test>
      <dependency group='org.gmock' name='gmock' version='0.8.2' />
      <dependency group='org.codehaus.geb' name='geb-spock' version='0.7.0' />
      <dependency group='org.seleniumhq.selenium' name='selenium-firefox-driver' version='2.21.0' />
    </test>
    <runtime>
      <dependency group='org.lesscss' name='lesscss' version='1.3.0' />
    </runtime>
  </dependencies>
  <plugins />
  <runtimePluginRequirements>
    <plugin name='resources' version='1.1.6 &gt; *' />
  </runtimePluginRequirements>
  <behavior />
</plugin>